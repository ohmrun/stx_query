{
  "build" : {
    "-D" : ["no-deprecation-warnings"],
    "-cp" : "src/main/haxe",
    "-lib" : [
      "stx_pico","stx_nano","stx_fn","stx_assert","stx_log",
      "stx_asys", "stx_om","stx_schema","stx_parse",
      "stx_ds_xset","stx_show", 
      "pml","hscript"
    ],
    "unit" : {
      "--macro" : [
        "include('stx.query',true)",
        "include('stx.assert.query',true)"
      ]
    },
    "test" : {
      "--connect" : "7070",
      "-L"       : "stx_test",
      "-cp"      : "src/test/haxe",
      "-main"    : "stx.query.Test",
      "interp": {
        "--interp" : true
      }
    },
    "feat"   : {
      "-L"       : "stx_test",
      "-cp"      : "src/test/haxe",
      "object_query" : {
        "-main" : "stx.query.term.object.Test",
        "interp" : {
          "--interp" : "true"
        } 
      }
    }
  },
  "run" : {
    
  }
}